public class fibinochi_at_nth {
    static  int fibi(int n){
        if(n<=1){
            return n;
        }
        int last=fibi(n-1);
        int secondlast=fibi(n-2);
        return last + secondlast;
    }

    public static void main(String[] args) {
        int n =4;
        System.out.println(fibi(n));
    }
}
