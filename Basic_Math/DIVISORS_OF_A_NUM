public class PRINT_ALL_DIVISORS {
    static void printDivisors(int a){
        for(int i=1;i<=Math.sqrt(a);i++){
            if(a%i==0){
                System.out.print(i+" ");
                if(i!=a/i){
                    System.out.print(a/i+" ");
                }
            }
        }
    }
    public static void main(String[] args) {
        int number=36;
        printDivisors(number);
    }
}
